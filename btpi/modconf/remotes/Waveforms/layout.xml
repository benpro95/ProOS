<?xml version="1.0" encoding="utf-8"?>
<layout>
    <row>
        <button text="--- Sinusoidal ---" color="#00004c" />
    </row> 
    <row>
        <button text="300Hz" ontap="sine300" color="#00004c" />
        <button text="600Hz" ontap="sine600" color="#00004c" />
        <button text="1KHz" ontap="sine1k" color="#00004c" />
    </row>
    <row>
        <button text="--- Square (no alias) ---" color="#00004c" />
    </row>     
    <row>
        <button text="300Hz" ontap="square300" color="#00004c" />
        <button text="600Hz" ontap="square600" color="#00004c" />
        <button text="1KHz" ontap="square1k" color="#00004c" />
    </row>
    <row>
        <button icon="pause" ontap="stop" color="#000000" />
    </row>
</layout>
